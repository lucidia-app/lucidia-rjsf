{
  "branches": [
    "main"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator", 
    "@semantic-release/changelog",
    [
      "@semantic-release/git",
      {
        "assets": "./CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/exec", {
        "publishCmd" : "yarn build && yarn publish --tag ${nextRelease.version}"
      }
    ]
  ],
  "repositoryUrl": "git@github.com:lucidia-app/lucidia-rjsf.git"
}